{
    "name": "brunocfalcao/laravel-vimeo-client",
    "description": "Laravel Vimeo Client - Enhanced",
    "keywords": [
        "brunocfalcao",
        "laravel",
        "vimeo"
    ],
    "homepage": "https://github.com/brunocfalcao/brunocfalcao/laravel-vimeo-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brunocfalcao\\VimeoClient\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brunocfalcao\\VimeoClient\\VimeoClientServiceProvider"
            ],
            "aliases": {
                "VimeoClient": "Brunocfalcao\\VimeoClient\\VimeoClientFacade"
            }
        }
    }
}
